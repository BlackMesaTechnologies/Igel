<!ATTLIST text xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST text
 %att.global.attribute.xmlid;
 %att.global.linking.attribute.corresp;
 %att.global.linking.attribute.next;
 %att.global.linking.attribute.prev; >
<!--doc:(text classification) groups information which describes the nature or topic of a text in terms of a standard classification scheme, thesaurus, etc. [2.4.3. ] -->
<!ELEMENT textClass (classCode|keywords)*>
<!ATTLIST textClass xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST textClass
 %att.declarable.attributes; >
<!--doc:contains a title for any kind of work. [3.11.2.2.  2.2.1.  2.2.5. ] -->
<!ELEMENT title %macro.paraContent;>
<!ATTLIST title xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST title
 %att.global.attribute.xmlid;
 %att.global.attribute.rendition;
 %att.global.linking.attribute.corresp;
 %att.global.linking.attribute.next;
 %att.global.linking.attribute.prev;
n CDATA  #IMPLIED
type (main|sub|volume|part) #IMPLIED
level (m|a|j|s) #IMPLIED >
<!--doc:(title page) contains the title page of a text, appearing within the front or back matter.  [4.6. ] -->
<!ELEMENT titlePage ((%model.global;)*,(%model.titlepagePart;),(%model.titlepagePart;|%model.global;)*)>
<!ATTLIST titlePage xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST titlePage
 %att.global.attribute.xmlid;
 %att.global.attribute.xmllang;
 %att.global.attribute.rendition;
 %att.global.linking.attribute.corresp;
 %att.global.linking.attribute.next;
 %att.global.linking.attribute.prev;
type (main|halftitle|series) #REQUIRED >
<!--doc:contains a subsection or division of the title of a work, as indicated on a title page. [4.6. ] -->
<!ELEMENT titlePart %macro.paraContent;>
<!ATTLIST titlePart xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST titlePart
 %att.global.attribute.xmlid;
 %att.global.attribute.xmllang;
 %att.global.attribute.rendition;
 %att.global.linking.attribute.corresp;
 %att.global.linking.attribute.next;
 %att.global.linking.attribute.prev;
type (main|sub|volume|desc|series|copyright|dedication|price) #REQUIRED >
<!--doc:(title statement) groups information about the title of a work and those responsible for its content. [2.2.1.  2.2. ] -->
<!ELEMENT titleStmt (title+,(%model.respLike;)*)>
<!ATTLIST titleStmt xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!--doc:contains a closing title or footer appearing at the end of a division of a text.  [4.2.4.  4.2. ] -->
<!ELEMENT trailer %macro.phraseSeq;>
<!ATTLIST trailer xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!ATTLIST trailer
 %att.global.attribute.xmlid;
 %att.global.attribute.xmllang;
 %att.global.attribute.rendition;
 %att.global.linking.attribute.corresp;
 %att.global.linking.attribute.next;
 %att.global.linking.attribute.prev; >
<!--doc:contains a description of the typefaces or other aspects of the printing of an incunable or other printed source. [10.7.2.1. ] -->
<!ELEMENT typeDesc (%model.pLike;)+>
<!ATTLIST typeDesc xmlns CDATA "http://www.tei-c.org/ns/1.0">
<!-- end elements -->
